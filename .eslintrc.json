{
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "plugins": [
      "react"
    ],
    "rules": {
      "react/jsx-uses-vars": "error",
      "react/jsx-uses-react": "error"
    },
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "tsx": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    }
  }